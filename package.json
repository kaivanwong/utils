{
  "name": "@kaivanwong/utils",
  "version": "0.0.1",
  "description": "JavaScript/TypeScript utils commonly used by @kaivanwong",
  "author": "Kaivan Wong <kaivanwong@outlook.com>",
  "keywords": [
    "kaivanwong",
    "utils"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kaivanwong/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaivanwong/utils.git"
  },
  "bugs": {
    "url": "https://github.com/kaivanwong/utils/issues"
  },
  "packageManager": "pnpm@7.21.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "rollup -c ./rollup.config.js -w",
    "build": "rollup -c ./rollup.config.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "rollup": "^3.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}